  "keywords": [
    "boilerplate",
    "react",
    "starter"
  ],
  "license": "MIT",
  "main": "src/index.tsx",
  "name": "react-starter",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kohded/react-starter.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "format": "prettier '**/*.{css,html,js,jsx,json,less,md,scss,ts,tsx,yml}' -c -w",
    "icons": "pwa-asset-generator ./src/assets/images/icons/logo.svg ./src/assets/images/icons -i ./src/index.html -f -m src/assets/static/manifest.webmanifest -o false -t png -v assets/images/icons",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:stylelint": "stylelint **/*.{css,less,scss}",
    "prebuild": "npm run format && npm run lint",
    "serve": "serve build -s --ssl-cert config/ssl/cert.pem --ssl-key config/ssl/key.pem",
    "setup": "npm run setup:husky && npm run setup:ssl",
    "setup:husky": "husky install && husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "setup:ssl": "cross-env mkdir -p config/ssl && mkcert -cert-file config/ssl/cert.pem -key-file config/ssl/key.pem localhost",
    "start": "cross-env NODE_ENV=development webpack serve"
  },
  "version": "0.1.0"
}
