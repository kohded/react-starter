  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "format": "prettier --config .prettierrc.js --write **/*.{css,html,js,jsx,json,less,md,scss,ts,tsx,yml}",
    "icons": "pwa-asset-generator ./src/assets/images/icons/logo.svg ./src/assets/images/icons -i ./src/index.html -f -m ./src/assets/build/manifest.webmanifest -o false -t png -v assets/images/icons",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint -c .eslintrc.js **/*.{js,jsx,ts,tsx}",
    "lint:stylelint": "stylelint --config .stylelintrc.js **/*.{css,less,scss}",
    "prebuild": "npm run format && npm run lint",
    "ssl": "cross-env mkdir -p config/ssl && mkcert -cert-file config/ssl/cert.pem -key-file config/ssl/key.pem localhost",
    "start": "cross-env NODE_ENV=development webpack serve"
  },
  "version": "0.1.0"
}
